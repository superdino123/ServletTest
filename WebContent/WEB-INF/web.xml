<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <context-param>
	<param-name>settingOne</param-name>
	<param-value>foo</param-value>
  </context-param>
  <context-param>
	<param-name>settingTwo</param-name>
	<param-value>bar</param-value>
  </context-param>
  
  <servlet>
  	<servlet-name>helloServlet</servlet-name>
  	<servlet-class>com.blog.HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>helloWorldServlet</servlet-name>
  	<servlet-class>com.blog.HelloWorldServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>servletParameterServlet</servlet-name>
  	<servlet-class>com.blog.ServletParameterServlet</servlet-class>
  	<init-param>
  		<param-name>database</param-name>
  		<param-value>CustomerSupport</param-value>
  	</init-param>
  	<init-param>
  		<param-name>server</param-name>
  		<param-value>10.0.12.5</param-value>
  	</init-param>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>helloServlet</servlet-name>
  	<url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>helloWorldServlet</servlet-name>
  	<url-pattern>/helloWorld</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>servletParameterServlet</servlet-name>
  	<url-pattern>/servletParameters</url-pattern>
  </servlet-mapping>
  
</web-app>
